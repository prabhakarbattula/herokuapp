<table class="table table-condensed sortable table-bordered">
  <thead>
      <th>Name</th>
      <th>Winner</th>
      <th data-defaultsort="desc">Votes</th>
      <th>Runner Up</th>
      <th>Votes</th>
      <th></th>
  </thead>
  <tbody>
    <% @seats.each do |mla| %>
      <tr>
        <td><%= mla.name %></td>
        <td><%= mla.winner.candidate %>(<%= link_to mla.winner.party.abbr, mla.winner.party %>)</td>
        <td><%= mla.winner.votes %></td>
        <td><%= mla.runner.candidate %>(<%= link_to mla.runner.party.abbr, mla.runner.party %>)</td>
        <td><%= mla.runner.votes %></td>
        <td><%= link_to 'Others', mla %></td>
      </tr>
    <% end %>
  </tbody>
</table>
